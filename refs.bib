	number = {1},
	urldate = {2022-04-04},
	journal = {The Auk},
	author = {Fitzgerald, Alyssa M and Weir, Jason and Ralston, Joel and Warkentin, Ian G and Whitaker, Darroch M and Kirchman, Jeremy J},
	month = jan,
	year = {2020},
	pages = {ukz066},
	file = {Full Text:/home/brandon/Zotero/storage/STM4YF6F/Fitzgerald et al. - 2020 - Genetic structure and biogeographic history of the.pdf:application/pdf},
}

