@article{staicer_singing_2006,
	title = {Singing behavior varies with breeding status of american redstarts (setophaga ruticilla)},
	volume = {118},
	issn = {1559-4491, 1938-5447},
	url = {http://www.bioone.org/doi/abs/10.1676/05-056.1},
