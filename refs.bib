	file = {Sólymos et al. - 2018 - Evaluating time-removal models for estimating avai.pdf:/home/brandon/Zotero/storage/METQEMNG/Sólymos et al. - 2018 - Evaluating time-removal models for estimating avai.pdf:application/pdf},
