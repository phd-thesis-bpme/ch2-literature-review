	pages = {duaa007},
	file = {Sólymos et al. - 2020 - Lessons learned from comparing spatially explicit .pdf:/home/brandon/Zotero/storage/BPPP3J9R/Sólymos et al. - 2020 - Lessons learned from comparing spatially explicit .pdf:application/pdf},
